<?xml version="1.0" encoding="utf-8" ?>
<presets author="Nikolaos Vastardis" version="0.0.1"
         description="S-201 field additions to the Seamark INT-1-preset.xml"
         shortdescription="S-201 field additions">

    <chunk id="shackleType">
        <list_entry value="forelock_shackles" display_value="forelock shackles"/>
        <list_entry value="clenching_shackles" diplay_value="clenching shackles"/>
        <list_entry value="bolt_shackles" diplay_value="bolt shackles"/>
        <list_entry value="screw_pin_shackles" diplay_value="screw pin shackles"/>
        <list_entry value="kenter_shackle" diplay_value="kenter shackle"/>
        <list_entry value="quick_release_link" diplay_value="quick release link"/>
    </chunk>

    <chunk id="categoryOfPowerSource">
        <list_entry value="battery" display_value="battery"/>
        <list_entry value="generator" display_value="generator"/>
        <list_entry value="solar-panel" display_value="solar-panel"/>
        <list_entry value="electrical-service" display_value="electrical-service3"/>
    </chunk>

    <group name="S: Electronic Position-Fixing Systems">

        <item name="Power Source (S7)">
            <space/>
            <key key="seamark:type" value="power_source"/>
            <combo key="s100:aidsToNavigation:power_source:category_of_power_source" text="Category of Power Source">
                <reference ref="categoryOfPowerSource"/>
            </combo>
            <text key="s100:aidsToNavigation:power_source:manufacturer" text="Manufacturer" type="text"/>
            <combo key="seamark:status" text="Status" default="">
                <reference ref="atonStatus"/>
            </combo>|
        </item>

    </group>

    <item name="Supplementary Information" type="node,way,closedway,relation">
        <text key="s100:aidsToNavigation:id_code" text="ID Code" type="text"/>
        <text key="s100:aidsToNavigation:date_end" text="Date End" type="date"/>
        <text key="s100:aidsToNavigation:date_start" text="Date Start" type="date"/>
        <text key="s100:aidsToNavigation:period_end" text="Period End" type="date"/>
        <text key="s100:aidsToNavigation:period_start" text="Period Start" type="date"/>
        <text key="s100:aidsToNavigation:source_date" text="Date End" type="date"/>
        <text key="s100:aidsToNavigation:scale_minimum" text="Scale Minimum" type="int"/>
        <text key="s100:aidsToNavigation:source_indication" text="Info" type="text"/>
        <text key="s100:aidsToNavigation:pictorial_representation" text="Pictorial Repr" type="text"/>
        <text key="s100:aidsToNavigation:inspection_frequency" text="Inspection Freq" type="text"/>
        <text key="s100:aidsToNavigation:inspection_requirements" text="Inspection Reqs" type="text"/>
        <text key="s100:aidsToNavigation:aton_maintenance_record" text="Maintenance Rec" type="text"/>
        <text key="s100:aidsToNavigation:installation_date" text="Installation Date" type="date"/>
        <text key="s100:aidsToNavigation:seasonal_action_required" text="Seasonal Action" type="text"/>
        <combo key="seamark:status" text="Status" default="">
            <reference ref="atonStatus"/>
        </combo>|
    </item>

</presets>
